{
    "name": "kalyabin/yii2-wysibb",
    "description": "Implements WysiBB to your Yii2 application",
    "version": "dev-master",
    "keywords": [
        "yii2",
        "framework",
        "wysiwig",
        "wysibb"
    ],
    "type": "package",
    "license": "BSD-3-Clause",
    "source": {
        "url": "git://github.com/kalyabin/yii2-wysibb.git",
        "type": "git",
        "reference": "master"
    },
    "authors": [
        {
            "name": "Max Kalyabin",
            "email": "maksim@kalyabin.ru",
            "homepage": "http://github.com/kalyabin/"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "bower-asset/jqjquery-wysibb": "~1"
    },
    "autoload": {
        "psr-0" : {
            "kalyabin\\wysibb" : "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
